import cv2
import numpy as np
img=np.zeros((512,512,3),np.uint8)

cv2.line(img,(0,0),(512,512),(0,255,0),5)

cv2.rectangle(img,(0,0),(200,100),(0,0,255),3)
cv2.circle(img,(200,50),50,(255,255,0),4)
cv2.putText(img,"opencv",(180,250),cv2.FONT_HERSHEY_COMPLEX,2,(0,255,255),2)
cv2.imshow("image",img)

cv2.waitKey(0)